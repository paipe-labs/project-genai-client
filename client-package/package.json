{
  "name": "genai-network-client",
  "version": "1.0.0",
  "description": "",
  "main": "./public/index.js",
  "type": "module",
  "scripts": {
    "build": "yarn rm:public && yarn generate:schemas && tsc && tsc-alias",
    "rm:public": "rm -rf ./public",
    "rm:schemas": "rm -rf src/schemas",
    "generate:schemas": "yarn rm:schemas && mkdir src/schemas && yarn generate:taskSchema && yarn generate:taskResultSchema",
    "generate:taskSchema": "tsx src/buildSchema.ts -o task | jq --sort-keys > src/schemas/task_schema.json",
    "generate:taskResultSchema": "tsx src/buildSchema.ts -o taskResult | jq --sort-keys > src/schemas/task_result_schema.json",
    "lint": "eslint --debug ./src",
    "lint:write": "eslint --debug ./src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|md)\"",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "author": "GenAI",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "browser-or-node": "^2.1.1",
    "comfy-ui-client": "^0.1.2",
    "commander": "^11.1.0",
    "json-schema-to-typescript": "^13.1.2",
    "jsonwebtoken": "^9.0.0",
    "node-sd-webui": "^0.0.8",
    "queue": "^7.0.0",
    "reconnecting-websocket": "^4.4.0",
    "tsc-alias": "^1.8.8",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "ws": "^8.13.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.3",
    "@types/ws": "^8.5.4",
    "dotenv": "^16.1.2",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8"
  }
}
