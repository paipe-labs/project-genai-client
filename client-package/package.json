{
  "name": "genai-network-client",
  "version": "1.0.0",
  "description": "",
  "main": "./public/index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node-esm --files -r tsconfig-paths/register ./src/run.ts",
    "test:comfyui": "cross-env NODE_ENV=production ts-node-esm --files -r tsconfig-paths/register ./src/testRun.ts",
    "watch": "cross-env ts-node-dev --files --respawn --inspect -r tsconfig-paths/register ./src",
    "build": "yarn rm:public && cross-env NODE_ENV=production tsc && tsc-alias",
    "rm:public": "rm -rf ./public",
    "copy:files": "cp .env ./public/.env",
    "lint": "eslint --debug ./src",
    "lint:write": "eslint --debug ./src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|md)\"",
    "prettier:write": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "author": "GenAI",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "browser-or-node": "^2.1.1",
    "comfy-ui-client": "^0.1.2",
    "commander": "^11.1.0",
    "jsonwebtoken": "^9.0.0",
    "node-sd-webui": "^0.0.8",
    "queue": "^7.0.0",
    "ws": "^8.13.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.3",
    "@types/ws": "^8.5.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.2",
    "eslint": "^8.41.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}